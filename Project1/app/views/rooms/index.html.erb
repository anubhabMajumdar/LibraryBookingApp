<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

  <ul class="list-group">


    <% @rooms.each do |room| %>
        <li class="list-group-item">
          <%= room.room_id %>&nbsp&nbsp&nbsp&nbsp
          <%= room.building %>&nbsp&nbsp&nbsp&nbsp
          <%= link_to 'Details', room %>&nbsp&nbsp&nbsp&nbsp
          <%= link_to 'Edit', edit_room_path(room) %>&nbsp&nbsp&nbsp&nbsp
          <%= link_to 'Delete', room, method: :delete, data: { confirm: 'Are you sure?' } %>
          <%= link_to 'View_room_history', redirect_to view_room_history_path %>
      </li>
    <% end %>


  </ul>


<%= link_to 'Return to manage rooms', admin_manage_room_url %></table>